<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean class="org.springframework.beans.factory.annotation.AutowiredAnnotationBeanPostProcessor"/>

    <bean id="helloBean" class="com.github.Karina_Denisevich.spring.hello_world.core.HelloWorld">
        <property name="value" value="Hello world"/>
        <property name="value1" value=" value1"/>
    </bean>

    <bean id="Person" class="com.github.Karina_Denisevich.spring.hello_world.core.Person">
        <property name="name" value="karina"/>
    </bean>

</beans>
